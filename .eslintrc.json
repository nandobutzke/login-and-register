{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "linebreak-style": "off",
      "react/react-in-jsx-scope": "off",
      "import/prefer-default-export": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-no-bind": [0, {
        "ignoreDOMComponents": 0,
        "ignoreRefs": 0,
        "allowArrowFunctions": 0,
        "allowFunctions": 0,
        "allowBind": 0
      }]
    }
}
