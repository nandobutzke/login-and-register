{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "linebreak-style": "off",
      "class-methods-use-this": "off",
      "function-paren-newline": "off",
      "consistent-return": "off"
    }
}
